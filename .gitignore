# Ignore directories generated by Composer
/drupal/.editorconfig
/.gitattributes
/vendor/
/drupal/web/_ping.php
/drupal/web/.gitignore
/drupal/web/core/
/drupal/web/modules/.gitignore
/drupal/web/modules/contrib/
/drupal/web/themes/.gitignore
/drupal/web/themes/contrib/
/drupal/web/profiles/.gitignore
/drupal/web/profiles/contrib/
/drupal/web/libraries/
/drupal/web/sites/.gitignore
/drupal/web/sites/default/.gitignore

# Ignore local configuration
/drupal/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/drupal/web/sites/*/files/

# Ignore testing directories.
/drupal/web/sites/simpletest
/drupal/web/tmp

# Ignore files generated by PhpStorm
/drupal/.idea/
/.idea/

# Ignore files generated by OS
/drupal/.DS_Store

# Ignore .env files as they are personal
/drupal/.env
/next/.env
/next/.env.local

# Ignore node modules
/drupal/node_modules
/next/node_modules

# Ignore oauth keys
/drupal/oauth
